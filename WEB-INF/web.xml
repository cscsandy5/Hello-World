<web-app>

  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>Index</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/Index</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>/Index</welcome-file>
  </welcome-file-list>

  <error-page>
    <error-code>404</error-code>
    <location>/Index</location>
  </error-page>

</web-app>
